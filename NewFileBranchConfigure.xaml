                    <ComboBox.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform Angle="0.035"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </ComboBox.RenderTransform>
                </ComboBox>
                <Button Content="完成" HorizontalAlignment="Left" Margin="144,399,0,0" VerticalAlignment="Top" Height="29" Width="56"/>
                <Button Content="关闭" HorizontalAlignment="Left" Margin="240,399,0,0" VerticalAlignment="Top" Height="29" Width="56"/>
            </Grid>

        </GroupBox>


    </Grid>
</Page>
