                <Label Content="X" HorizontalAlignment="Left" Margin="135,0,0,0" VerticalAlignment="Center"/>
                <Label Content="Y" HorizontalAlignment="Left" Margin="224,0,0,0" VerticalAlignment="Center"/>

                <Label Content="测 高  点" HorizontalAlignment="Left" Margin="10,240,0,0" VerticalAlignment="Top"/>
                <Label Content="高 度  值" HorizontalAlignment="Left" Margin="10,270,0,0" VerticalAlignment="Top"/>
                <Label Content="最大差值" HorizontalAlignment="Left" Margin="10,300,0,0" VerticalAlignment="Top"/>

                <Label Content="连续加速度" HorizontalAlignment="Left" Margin="10,330,0,0" VerticalAlignment="Top"/>
                <Label Content="拐弯时间" HorizontalAlignment="Left" Margin="10,360,0,0" VerticalAlignment="Top"/>
                <TextBox HorizontalAlignment="Left" Margin="123,244,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="63"/>
                <TextBox HorizontalAlignment="Left" Margin="208,244,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="63"/>

                <TextBox HorizontalAlignment="Left" Margin="123,274,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="63"/>
                <Button Content="读高度" HorizontalAlignment="Left" Margin="208,281,0,0"  VerticalAlignment="Top" Width="63" Height="25"/>

                <TextBox HorizontalAlignment="Left" Margin="123,304,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="63"/>

                <TextBox HorizontalAlignment="Left" Margin="123,334,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="63"/>

                <TextBox HorizontalAlignment="Left" Margin="123,364,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="63"/>
                <Button Content="读测高位置" HorizontalAlignment="Left" Margin="309,245,0,0" VerticalAlignment="Top" Width="63" Height="25"/>
                <Button Content="移动相机" HorizontalAlignment="Left" Margin="309,280,0,0" VerticalAlignment="Top" Width="63" Height="25"/>
                <Button Content="移动laser" HorizontalAlignment="Left" Margin="309,315,0,0" VerticalAlignment="Top" Width="63" Height="25"/>
