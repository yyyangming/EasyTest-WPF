<Page x:Class="Test.NewFileBranchConfigure"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Test"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="500"
      Title="NewFileBranchConfigure">

    <Grid>
        <GroupBox Header="子程序参数">
            <Grid>
                <GroupBox Header="子程序基准点" Margin="0,90,-2,0" Height="97" VerticalAlignment="Top">
                    <Grid Margin="0,0,-12,0">
                        <Label Content="X" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="32,0,0,0"/>
                        <Label Content="Y" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="32,37,0,0"/>
                        <TextBox HorizontalAlignment="Left" Margin="77,4,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
                        <TextBox HorizontalAlignment="Left" Margin="77,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
                        <Button Content="移动" HorizontalAlignment="Left" Height="25" Margin="245,7,0,0" VerticalAlignment="Top" Width="63"/>
                        <Button Content="移动" HorizontalAlignment="Left" Height="25" Margin="245,37,0,0" VerticalAlignment="Top" Width="63"/>
                    </Grid>

                </GroupBox>
                <Label Content="编程模式" HorizontalAlignment="Left" Margin="30,45,0,0" VerticalAlignment="Top"/>
                <TextBox HorizontalAlignment="Left" Margin="91,19,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
