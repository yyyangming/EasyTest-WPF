<Window x:Class="Test.NewFile2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Test"
      mc:Ignorable="d" 
      Height="460" 
      Width="800" ResizeMode="NoResize"
      Title="参数" Loaded="NewFile2_JogPage_Loaded" Topmost="True">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="500"></ColumnDefinition>
            <ColumnDefinition Width="300"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid Height="380" Margin="0 -30 0 0">
            <ContentControl Name="NewFileConfigure">
            </ContentControl>
        </Grid>
        <Frame Grid.Column="1" Name="JogPage" Margin="0 -20 0 0"></Frame>
        <Button FontSize="15" Width="50" Content="上一步"  HorizontalAlignment="Left" Margin="356,388,0,0" Padding="0" VerticalAlignment="Top" Height="20" Click="btnBeforePage_Click"/>
        <Button FontSize="15" Width="50" Click="btnNextPage_Click" Content="确定" HorizontalAlignment="Left" Margin="445,388,0,0" Padding="0" VerticalAlignment="Top" Height="20" Cursor="Hand"/>
    </Grid>
</Window>
