<Window x:Class="Test.RobotSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Test"
        mc:Ignorable="d"
        Title="RobotSettings" Height="200" Width="500">
    <Grid>
        <DataGrid Name="DATA_Rabbit" ItemsSource="{Binding}"  AutoGenerateColumns="False" Margin="0,0,0,54" Loaded="DATA_Rabbit_loaded">
            <DataGrid.Columns>
                <DataGridTextColumn Width="250" Header="属性" Binding="{Binding property}"></DataGridTextColumn>
                <DataGridTextColumn Width="250" Header="值" Binding="{Binding value}"></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>
        <Button Content="OK" HorizontalAlignment="Left" Margin="171,146,0,0" VerticalAlignment="Top" Width="41"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="293,146,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
